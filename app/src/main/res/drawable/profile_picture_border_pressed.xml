<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">
            <solid android:color="@color/ent_background_gray" />
            <stroke android:width="1dp" android:color="#555555" />
            <padding android:left="0dp" android:top="0dp" android:right="1dp" android:bottom="1dp" />
        </shape>
    </item>
    <item>
        <shape
            android:shape="oval">
            <solid android:color="@color/ent_background_blue" />
            <stroke android:width="2dp" android:color="@color/ent_foreground_blue" />
            <padding android:left="2dp" android:top="2dp" android:right="2dp" android:bottom="2dp" />
        </shape>
    </item>
</layer-list>